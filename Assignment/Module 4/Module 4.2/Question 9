#include<iostream>
using namespace std;
class overloading
{
    public:
    int data(int a , int b)
    {
        cout<<"Addition is "<<a+b<<endl;
        return 0;
    }
    int data(double a, double b)
    {
        cout<<"Subtraction is "<<a-b<<endl;
        return 0;
    }
    int data(int a, double b)
    {
        cout<<"Multiplication is "<<a*b<<endl;
        return 0;
    }
    int data(double a, int b)
    {
        cout<<"Multiplication is "<<a/b<<endl;
        return 0;
    }
};
int main()
{
    overloading obj;
    obj.data(10,20);
    obj.data(10.1 ,20.1);
    obj.data(10 ,20.1);
    return 0;
   
}
