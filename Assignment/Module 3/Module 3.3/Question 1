#include<stdio.h>
int maximum(int []);
int main()
{
    int a[10],i,m;
    printf("\nEnter 10 number:");
    for(i=0;i<10;i++)
    scanf("%d",&a[i]);
    m=maximum(a);
    printf("\nmaximum=%d",m);
    return 0;
}
int maximum(int a[])
{
    int i,m=-32768;
    for(i=0;i<10;i++){
        if(a[i]>m)
        m=a[i];
    }
    return 0;
}
