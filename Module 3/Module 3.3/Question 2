#include<stdio.h>
int main(){
    int a,b,c,choice;
    printf("Enter first number:");
    scanf("%d",&a);
    printf("Enter second number:");
    scanf("%d",&b);

    printf("\n1 for Addition");
    printf("\n2 for Subtraction");
    printf("\n3 for Division");
    printf("\n4 for Multiplication");

    printf("\n\nEnter your choice:");
    scanf("%d",&choice);

    switch(choice)
    {
        case 1:
          c=a+b;
          printf("Addition=%d",c);
        break;

        case 2:
          c=a-b;
          printf("Subtraction=%d",c);
        break;

        case 3:
          c=a/b;
          printf("division=%d",c);
        break;

        case 4:
          c=a*b;
          printf("Multiplication=%d",c);
        break;
    }
    return 0;
}
