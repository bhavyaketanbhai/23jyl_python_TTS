#include<stdio.h>

int main(){
    char ch;
   
    printf("Enter any Alphabet:");
    scanf("%c",&ch);
    if((ch>='A' &&ch<='Z') ||(ch>='a' &&ch<='z'))
    {
         switch(ch){
               case 'A':
                 case 'E':
                   case 'I':
                     case 'O':
                       case 'U':
                case 'a':
                   case 'e':
                     case 'i':
                       case 'o':
                         case 'u':
                           case '0':
                printf("\n %c is vowel..",ch);
                break;
                defaut:
                printf("\n %c is consonant..",ch);
         }
    }
    else
    printf("\n %c is not alphabet:",ch);
    return 0; 
}
